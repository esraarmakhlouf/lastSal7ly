@model ChangePassword
@{
    ViewBag.Title = "تغيير كلمة المرور";
    Layout = "_Authenticate";
}


<div class="container forget-password">
    <div class="row">
        <div class="col-md-12 col-md-offset-4">
            <div class="panel panel-default">
                <div class="panel-body">
                    <div class="text-center">
                        <img src="https://usa.afsglobal.org/SSO/SelfPasswordRecovery/images/send_reset_password.svg?v=3" alt="Reset Password" border="0">
                        <h3 class="text-center text-green">Reset Password Done </h3>
                        <p >Your Reset Password Done Check Your Email Address for Reset Mail.</p>
                        

                    </div>
                </div>
            </div>
        </div>
    </div>
</div>